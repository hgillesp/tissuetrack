<h1>Listing tissue_samples</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Id</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tissue_samples.each do |tissue_sample| %>
  <tr>
    <td><%= tissue_sample.description %></td>
    <td><%= tissue_sample.id %></td>
    <td><%= tissue_sample.user_id %></td>
    <td><%= link_to 'Show', tissue_sample %></td>
    <td><%= link_to 'Edit', edit_tissue_sample_path(tissue_sample) %></td>
    <td><%= link_to 'Destroy', tissue_sample, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

